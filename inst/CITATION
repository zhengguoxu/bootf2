citHeader("To cite package 'bootf2' in publications use:")

citEntry(entry = "Manual",
  title        = paste("bootf2: Simulation and Comparison of",
                       "Dissolution Profiles"),
  author       = personList(as.person("Zhengguo Xu")),
  year         = "2021",
  note         = "R package version 0.4.0",
  url          = "https://CRAN.R-project.org/package=bootf2",
  textVersion  =
  paste("Zhengguo Xu (2021). bootf2: Simulation and Comparison of",
        "Dissolution Profiles. R package version 0.4.0.",
        "URL https://CRAN.R-project.org/package=bootf2")
)
